{
  "name": "youtubeclone",
  "version": "1.0.0",
  "main": "dist/backend/server.js",
  "scripts": {
    "build:backend": "tsc -p tsconfig.backend.json",
    "build:frontend": "tsc -p tsconfig.frontend.json",
    "build": "npm run build:backend && npm run build:frontend",
    "start": "node dist/backend/server.js",
    "dev:backend": "nodemon --watch src/backend --exec ts-node src/backend/server.ts"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "multer": "^2.0.2",
    "sqlite3": "^5.1.7"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.3.3",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  }
}
